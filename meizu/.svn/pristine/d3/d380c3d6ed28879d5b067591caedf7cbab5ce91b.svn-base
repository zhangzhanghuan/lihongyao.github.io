@charset "utf-8";
@import "../../node_modules/swiper/dist/css/swiper.min.css";


// 阿里矢量图
@font-face {
    font-family: 'iconfont';  /* project id 856764 */
    src: url('//at.alicdn.com/t/font_856764_psnysdi2rbh.eot');
    src: url('//at.alicdn.com/t/font_856764_psnysdi2rbh.eot?#iefix') format('embedded-opentype'),
    url('//at.alicdn.com/t/font_856764_psnysdi2rbh.woff') format('woff'),
    url('//at.alicdn.com/t/font_856764_psnysdi2rbh.ttf') format('truetype'),
    url('//at.alicdn.com/t/font_856764_psnysdi2rbh.svg#iconfont') format('svg');
  }
.iconfont{
    font-size: 23px;
    color: #888888;
    line-height: 35px;
}
body{
    width: 100%;
    margin: 0 auto;
    background: #F2F2F2;
    min-width: 1100px;
    height: auto;
}
.sidebar .store .icon-top{
    transform: rotateZ(-90deg);
    margin-top: 15px;
    font-size: 35px;
}
.header{
    width: 100%;
    height: 35px;
    background:#484848 ;
    display: inline-block;
}
.header .nav{
    line-height: 35px;
    width: 1050px;
    margin: 0 auto;

}
.nav a{
    display: inline-block;
    margin-right: 22px;
    color: #aaa;
    transition: all .2s linear;
    font-size: 14px;
}
.nav a:hover{
    color: white;
}
.last{
    float:right;
}
// 隐藏图层
.box{
    width: 1386px;
    height: 280px;
    z-index: 10;
    box-shadow: 0 2px 5px rgb(207, 205, 205);
    margin-left:-1125px;
    text-align: center;
    display: none;
    position: absolute;
    background: white;
}
.box img{
    width: 1050px;
    height:260px;
    margin-top:9px;
}
// 三角形
.san{
    width: 0;
    height: 0;
    border-width: 15px;
    border-style: solid;
    border-color:  transparent transparent white transparent;
    margin-top:-25px;
    margin-left:1165px;
    position: absolute;
}
.nav-1{
    width: 100%;
    height: 83px;
    background: white;
}
.nav-box{
    width: 1050px;
    margin: 0 auto;
    height: 83px;
   
    line-height: 83px;
}
.nav-box img{
    width: 140px;
    height: 25px;
}
.one-words{
    font-size: 20px;
    padding: 15px;
}
.nav-list{
    float: right;
    margin-right:280px;
    // display: inline-block;
    width: 419px;
}
.nav-list li{
    float: left;
    padding-right: 39px;
}
.nav-list li a{
    color: #515151;
}
.nav-list li a:hover{
    color: #00c3f5;
}
// 版块下拉列表
.ban-main{
    position: relative;
}
.ban{
    width: 480px;
    height: 255px;
    background:white;
    position: absolute;
    margin-left:-240px;
    margin-top:15px;
    box-shadow:  10px 5px white,
    0 10px 5px white,
    10px 0 5px white;
    z-index: 10;
    display: none;
}
.ul-list-community li{
    height: 10px;

}

.ul-list-community li a{
    font-size: 14px;
    padding-left: 25px;
}
.ul-list-community li a:hover{
    color: rgb(133, 125, 125)
}
.list {
    margin-top:-25px;
}
.list-1{
    margin-top: -50px;
}
.list-2{
    margin-top: -75px;
    
}
.san-1{
    width: 0;
    height: 0;
    border-width: 15px;
    border-style: solid;
    border-color:  transparent transparent white transparent;
    margin-top:-30px;
    margin-left:236px;
    position: absolute;
}

// 搜索
.search-box{
    display: inline-block;
    position: absolute;
    margin-left: 550px;
    height: 70px;
    
}
.search-box input{
    height: 30px;
    width: 200px;
    border: 1px solid #EEEEEE
}

.search-box .iconfont{
    font-size: 20px;
}
.search-box button{
    vertical-align: middle;
    background: white;

}
.search-box img{
    width: 36px;
    height: 36px;
}
.button i{
    font-size: 10px;
}
.hide-img{
    width: 100px;
    height: 200px;
    background:white;
    margin-left:200px;
    display: none;
    margin-top:-50px;
}
.login-box{
    margin-top:-10px;
}
.login-box li{
    margin-bottom: -40px;
    text-align: center;
}
.login-box li a{
    font-size: 12px;
    color: #a2a2a2;
}
.login-box li a:hover{
    color: #00c3f5;
}
.hide-img img{
    width: 30px;
    height: 30px;
    margin-top:-30px;
}
.hide-span{
    padding-left: 20px;
}

// 主要内容

.content{
    width:1050px;
    height:auto;
    margin:0 auto;
    margin-top:20px;
}
nav{
    width: 40%;
    height:55px;
    float: right;
}
nav li{
    margin-right:20px;
     float:left;
    line-height: 68px;
}
nav li a{
    color: #777;
    font-size: 14px;
}
.swiper-container{
    width: 100%;
    height: 295px;
}
.swiper-container img{
    width: 100%;
}

// 人物
.ul-list-photo{
    margin-top:30px;
    background: white;
    width: 1050px;
    height:321px;
   
}
.ul-list-photo li{
    width: 16.6%;
    height:321px;
    float: left;
    margin-top:20px;
    text-align: center;
}
.title-photo{
    margin-left:30px;
    width: 95%;
    height: 43px;
    border-bottom: 1px solid #d4d4d4;
    line-height: 50px;
    font-size: 16px;
    font-weight: 400;
    padding-bottom: 18px;
}
.ul-list-photo img{
    border-radius: 50%;
    width: 80px;height: 80px;
}
.author-name{
    padding: 8px 0;
    color: #000;
    font-size: 14px;
    text-align: center;
}
.sign {
    padding-bottom: 8px;
   width: 100%;
    height: 21px;
    color: #9e9e9e;
    font-size: 12px;
    text-align: center;
}
.do{
    display: inline-block;
    width: 100px;
    height: 30px;
    line-height: 30px;
    text-align: center;
    background: #00c3f5;
    color: #fff;
    font-size: 14px;
    border-radius: 3px;
    cursor: pointer;
}

// 图片
.photo-list{
    width: 100%;
    height:1100px;
    background: white;
    margin-top:20px;
    margin-bottom:20px;
}
.photo-list img{
    width:100%;
    height: 100%;
}
.photo-list li{
    float: left;
    width: 20%;
    margin-left:40px;
    position: relative;
    margin-top:20px;
    text-align: center;
}
.hover-info{
    display: none;
    position: absolute;
    width: 100%;
    height: 0;
    left: 0;
    top: 0;
    // opacity: 0.5;
    z-index: 2;
    background: rgb(0, 0, 0) ;
}

.hover-info a{
    color:white;
    font-size: 12px;
}
.hover-top{
    float:right;
}
.hover-bottom{
    margin-top:170px;
}
.photo-title{
    width: 95%;
    margin-bottom: 18px;
    margin-left:20px;
    padding-bottom: 18px;
    font-size: 16px;
    font-weight: 400;
    border-bottom: 1px solid #d4d4d4;
}