main{
    min-width: 1100px;
}
.con{
    background: #f6f8fb;
}
.container{
    width: 1240px;
    margin: 0 auto;
    padding: 10px 0;
}
.container{
    overflow: hidden;
}
.container span{
    font-size: 26px;
    float: left;
    margin-top: 15px;
}
.container>ul{
    float: right;
    margin-top: 26px;
    margin-right: -15px;
    li{
        float: left;
        border-right: 1px solid #dedede;
        a{
            display: block;
            padding: 0 20px;
            line-height: 20px;
            font-size: 16px;
            color: #333;
            text-decoration: none;
            transition: color .3s;
        }
    }
}
.left-container{
    float: left;
}
.container-row{
    width: 1240px;
    margin: 0 auto;
    overflow: hidden;
}
.container-img{
    width: 560px;
    position: relative;
    margin-right: 40px;
    height: 560px;
    img{
        width: 560px;
        height: 560px;
        position: absolute;
    }
}
.small-img{
    overflow: hidden;
    margin-top: 10px;
    margin-left: 24px;
    li{
        display: inline-block;
        margin: 0 24px;
        overflow: hidden;
        height: 102px
    }
}
.small-img {
    img{
        width: 80px;
        height: 80px;
    }
}
.right-container{
    position: relative;
    float: left;
    width: 640px;
    font-size: 12px;
    padding-top: 25px;
}
.con-clor{
    background: #fff;
}
.right-text{
    position: relative;
    padding-top: 30px;
    margin-bottom: 12px;
    h1{
        margin-bottom: 4px;
        font-size: 28px;
        line-height: 1.2;
        font-weight: 700;
        color: #262626;
    }
    p{
        font-size: 14px;
        color: #999;
    }
}
.right-price{
    position: relative;
    background: #f4f6fa;
    padding: 16px 0 12px;
    .money{
        color: #e02b41;
        font-size: 36px;
        font-weight: 500;
    }
}
.right-price-small{
    color: #e02b41;
    font-size: 24px;
}
.vm{
    span{
        margin-right: 20px;
        color: #8c8c8c;
        line-height: 20px;
    }
}
.div-server{
    border-bottom: 1px dashed #dedede;
    padding: 18px 0;
}