}


// 动画效果
@keyframes fadeIndown{
    from{
        opacity: 0;
        transform: translateY(60px);
    }
    to{
        opacity: 1;
        transform: translateY(0);
    }
}

@keyframes fadeInup{
    from{
        opacity: 0;
        transform: translateY(-60px);

    }
    to{
        opacity: 1;
        transform: translateY(0);
    }
}
.animation-ul li:nth-child(2n-1) .span-p1{
    margin-left:50px;
}
.animation-ul li{
    animation: fadeIndown 2s linear  paused;
}
.animation-ul li img{
    animation: fadeInup 2s linear paused;
}
/* 动画效果 */
.running .animation-item {
    animation-play-state: running;
}
