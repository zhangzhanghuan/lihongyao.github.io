@import "//at.alicdn.com/t/font_922338_lietr51awyr.css";
main{
    min-width: 1100px;
}
.con{
    background: #f6f8fb;
}
.container{
    width: 1240px;
    margin: 0 auto;
    padding: 10px 0;
}
.container{
    overflow: hidden;
}
.container span{
    font-size: 26px;
    float: left;
    margin-top: 15px;
}
.container>ul{
    float: right;
    margin-top: 26px;
    margin-right: -15px;
    li{
        float: left;
        border-right: 1px solid #dedede;
        a{
            display: block;
            padding: 0 20px;
            line-height: 20px;
            font-size: 16px;
            color: #333;
            text-decoration: none;
            transition: color .3s;
        }
    }
}
.left-container{
    float: left;
}
.container-row{
    width: 1240px;
    margin: 0 auto;
    overflow: hidden;
}
.container-img{
    width: 560px;
    position: relative;
    margin-right: 40px;
    height: 560px;
    img{
        width: 560px;
        height: 560px;
       
        position: absolute;
    }
}
.prz-wrapper{
    cursor: pointer;
}
.small-img{
    overflow: hidden;
    margin-top: 10px;
    margin-left: 24px;
    li{
        cursor: pointer;
        display: inline-block;
        margin: 0 24px;
        overflow: hidden;
        height: 102px
    }
}
.small-img {
    img{
        width: 80px;
        height: 80px;
    }
}
.right-container{
    position: relative;
    float: right;
    width: 640px;
    font-size: 12px;
    padding-top: 25px;
}
.con-clor{
    background: #fff;
}
.right-text{
    position: relative;
    padding-top: 30px;
    margin-bottom: 12px;
    h1{
        margin-bottom: 4px;
        font-size: 28px;
        line-height: 1.2;
        font-weight: 700;
        color: #262626;
    }
    p{
        font-size: 14px;
        color: #999;
    }
}
.right-price{
    position: relative;
    background: #f4f6fa;
    padding: 16px 0 12px;
    .money{
        color: #e02b41;
        font-size: 36px;
        font-weight: 500;
    }
}
.right-price-small{
    color: #e02b41;
    font-size: 24px;
}
.vm{
    span{
        margin-right: 10px;
        color: #8c8c8c;
        line-height: 20px;
    }
}
.vm span:nth-child(1){
    letter-spacing: 20px;
}
.div-server{
    border-bottom: 1px dashed #dedede;
    padding: 18px 0;
    margin-bottom: 15px;
}
.vm-server{
    margin-top: 15px;
    span:nth-child(1){
        width: 75px;
        font-size: 12px;
        line-height: 18px;
        color: #8c8c8c;
        margin-right: 30px;
    }
    span{ 
        height: 18px;
        min-width: 96px;
        line-height: 18px;
        font-size: 14px;
        color: #262626;
        cursor: pointer;
    }
    p{
        padding: 0;
        margin-top: -20px;
        margin: 0;
        padding-left:80px; 
    }
}
.type span{
    
    width: 75px;
    font-size: 12px;
    line-height: 42px;
    color: #8c8c8c;
}
.type-one{
   
}
.type-one span:nth-child(1){
    width: 65px;
    display: inline-block;
    letter-spacing: 20px;
}
.type span:nth-child(1){
    width: 65px;
    display: inline-block;
}
.type span:not(:nth-child(1)){
    width: 176px;
    text-overflow: ellipsis;
    white-space: nowrap;
    display: inline-block;
    margin: 0 10px 10px 0;
    background-color: #fff;
    white-space: nowrap;
    line-height: 40px;
    border: 1px solid #BFBFBF;
    border-radius: 2px;
    color: #595959;
    text-align: center;
    text-decoration: none;
    font-size: 14px
}
.type-color span:nth-child(4){
    margin-right: 0;
    margin-left: 0;
}
.type-color span:nth-child(5){
    margin-left: 68px;
}
.type-num{
    font-size: 0;
    margin-bottom: 45px;
}

.type-num span:nth-child(1){
    width: 65px;
    display: inline-block;
    letter-spacing: 20px;
    font-size: 12px;
    line-height: 42px;
    color: #8c8c8c;
}
.type-num span:nth-child(2),.type-num span:nth-child(4){
    display: inline-block;
    width: 32px;
    line-height: 42px;
    text-align: center;
    height: 40px;
    vertical-align: top;
    border: 1px solid #e7e7e7;
    background: #f7f7f7;
    color: #262626;
    font-size: 12px;
    cursor: pointer;
}
.type-num input{
    width: 60px;
    height: 40px;
    text-align: center;
    vertical-align: top;
    border: 1px solid #dedede;
}
.once{
    margin-top: 17px;
    margin-bottom: 40px;
}
.once button{
    padding: 15px 0;
    cursor: pointer;
    width: 208px;
    font-size: 20px;
    line-height: 24px;
    margin-right: 20px;
    color: #fff;
    border-radius: 3px;
   
}
.once button:nth-child(1){
    background-color: #27b3ff;
}
.once button:nth-child(2){
    border:1px solid #27b3ff;
    background-color: #fff;
    color: #27b3ff;
}