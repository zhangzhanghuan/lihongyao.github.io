header{
    min-width: 1100px;
    width: 100%;
    z-index: 998;
    height: 80px;
    background-color: #fff;
    border-bottom: 1px solid #efefef;
}
.wid{
    width: 1240px;
    margin: 0 auto;
}
main{
    background-color: #F4F6FA;
    padding-top: 15px;
    padding-bottom: 15px;
}
.logo{
    float: left;
    height: 80px;
    overflow: hidden;
    a{
        display: block;
        width: 150px;
        height: 80px;
        img{
            width: 100%;
            height: 23px;
            margin-top: 21px;
        }
    }
}
header{
    ul{
        float: left;
        list-style: none;
        li{
            float: left;
            color: #333;
            font-size: 14px;
        }
    }
}
.list{
    margin-top: 24px;
    margin-left: 15px;
}
.wid-right{
    float: right;
    li{
        padding:0 20px;
        font-size: 12px;
        line-height: 40px;
    }
}
.wid-border{
    overflow: hidden;
    background: #fff;
    border: 1px solid #efefef;
}
.check-box{
    width: 550px;
    float: left;
    text-align: left;
    padding: 13px 0;
    font-size: 14px;
    padding-left: 24px;
    .check-item{
        display: inline-block;
        vertical-align: middle;
        width: 18px;
        height: 18px;
        border: 1px solid #BDBDBD;
        border-radius: 2px;
        background-color: #fff;
        text-align: center;
        font-size: 10px;
        line-height: 17px;
        cursor: pointer;
        -webkit-user-select: none;
        -moz-user-select: none;
        -ms-user-select: none;
        user-select: none;
    }
    .check-tips{
        vertical-align: middle;
        font-size: 14px;
        color: #333;
    }
}
.wid-price{
    float: left;
    display: inline-block;
    padding: 19px 0 6px;
    color: rgba(51,51,51,.4);
    font-size: 12px;
    vertical-align: middle;
}
.price{
    width: 200px;
}
.num{
    width: 180px;
}
.total{
    width: 140px;
}
.edit{
    width: 90px;
    text-align: right;
    color: #00c3f5;
    padding-right: 48px;
    cursor: pointer;
}
.free{
    font-size: 12px;
    color: #8c8c8c;
    margin-left: 20px;
}
.box-free{
    background-color: #FAFAFC;
}
.wid-img{
    width: 100px;
    height: 100px;
    margin-left: 19px;
    display: inline-block;
    vertical-align: middle;
    img{
        width: 100%;
        height: 100%;
    }
}
ul{
    list-style: none;
    margin: 0;
    padding: 0;
}
.des{
    width: 330px;
    vertical-align: middle;
    display: inline-block;
    margin-left: 19px;
    text-decoration: none;
}
.des p{
    font-size: 16px;
    color: #262626;
}
.des p:nth-child(2){
    margin-top: 4px;
    color: #8C8C8C;
    font-size: 14px;
    width: 100%;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}
.cart-price{
    width: 220px;
    display: inline-block;
    text-align: center;
}
.wid-border>li{
    width: 100%;
}
.cart-price>span{
    width: 24px;
    color: #595959;
    display: inline-block;
}
.cart-input{
    width: 100px;
    font-size: 0;
    span{
        width: 24px;
        vertical-align: top;
        color: #595959;
        font-size: 14px;
        background-color: rgb(192, 188, 188);
    }
}
.cart-price input{
    width: 30px;
    height: 13px;
    text-align: center;
}
.not{
    width: 160px;
    text-align: center;
    display: inline-block;
}
.wid ul{
    margin-bottom: 20px;
}
.footer-num{
    float: right;
    margin-right: 30px;
    padding: 13px 0 13px 24px;
    span{
        font-size: 14px;
    }
}
.mid>span{
    vertical-align: middle;
}
.num-color{
    color: red;
}
.footer-num .big-color{
    font-size: 20px;
    font-weight: bolder;
    color: #E02B41;
    margin-right: 24px;
    vertical-align: middle;
}
.footer-num button{
    padding: 5px 20px;
    color: #fff;
    border-color: #DBDBDB;
    outline: none;
    border: none;
    cursor: pointer;
}